{   "name": "Rock, Paper, Scissors",
    "functions": {
        "vs": {"arguments": 2, "associative": false, "commutative": true, "alternativeNames": ["play"]}
    },
    "constants": {
        "rock": "",
        "paper": "",
        "scissors": ""
    },
    "actions": [
        {   "title": "Play"
        ,   "description": "Reveals the winner of a game"
        ,   "matches": [
            {"from": "\\vs(\\rock,\\paper)", "to": ["\\paper"]},
            {"from": "\\vs(\\rock,\\scissors)", "to": ["\\rock"]},
            {"from": "\\vs(\\scissors,\\paper)", "to": ["\\scissors"]}
            ]
        }
    ]
}