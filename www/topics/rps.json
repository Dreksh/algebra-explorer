{   "name": "Rock, Paper, Scissors",
    "functions": {
        "play": {"arguments": 2, "associative": false, "commutative": true}
    },
    "constants": {
        "rock": "",
        "paper": "",
        "scissors": ""
    },
    "actions": [
        {   "title": "Resolve"
        ,   "description": "Reveals the winner of a game"
        ,   "matches": [
            {"from": "\\play(\\rock,\\paper)", "to": ["\\paper"]},
            {"from": "\\play(\\rock,\\scissors)", "to": ["\\rock"]},
            {"from": "\\play(\\scissors,\\paper)", "to": ["\\scissors"]}
            ]
        }
    ]
}